<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Excercise1"
        tools:targetApi="31">
        <activity
            android:name=".activity.CheckoutHomeActivity"
            android:exported="false" />
        <activity
            android:name=".activity.TownhouseHomeTypeActivity"
            android:exported="false"
            android:parentActivityName=".activity.HomeTypesActivity" />
        <activity
            android:name=".activity.CondoHomeTypeActivity"
            android:exported="false"
            android:parentActivityName=".activity.HomeTypesActivity" />
        <activity
            android:name=".activity.SemiDetachedHomeTypeActivity"
            android:exported="false"
            android:parentActivityName=".activity.HomeTypesActivity" />
        <activity
            android:name=".activity.DetachedHomeTypeActivity"
            android:exported="false"
            android:parentActivityName=".activity.HomeTypesActivity" />
        <activity
            android:name=".activity.ApartmentHomeTypeActivity"
            android:exported="false"
            android:parentActivityName=".activity.HomeTypesActivity" />
        <activity
            android:name=".activity.HomeTypesActivity"
            android:exported="false" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Excercise1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>